[run]
branch = True
omit =
    */migrations/*
    user_guide/version.py
    user_guide/apps.py
source = user_guide
[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError
fail_under = 100
show_missing = 1
